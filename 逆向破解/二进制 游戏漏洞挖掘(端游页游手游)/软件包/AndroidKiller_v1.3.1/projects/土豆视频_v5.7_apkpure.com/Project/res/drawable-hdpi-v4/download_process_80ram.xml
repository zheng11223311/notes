<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <gradient android:startColor="#ffefefef" android:endColor="#ffefefef" android:angle="270.0" android:centerY="0.75" android:centerColor="#ffefefef" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <gradient android:startColor="#ffff6600" android:endColor="#ffff6600" android:angle="270.0" android:centerY="0.75" android:centerColor="#ffff6600" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="#ffff6600" android:endColor="#ffff6600" android:angle="270.0" android:centerY="0.75" android:centerColor="#ffff6600" />
            </shape>
        </clip>
    </item>
</layer-list>