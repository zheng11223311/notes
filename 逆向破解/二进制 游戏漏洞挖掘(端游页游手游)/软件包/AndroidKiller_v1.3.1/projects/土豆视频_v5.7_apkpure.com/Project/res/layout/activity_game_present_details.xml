<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:id="@id/present_detail_parent" android:background="#ffe2e2e2" android:layout_width="fill_parent" android:layout_height="fill_parent"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <RelativeLayout android:id="@id/present_content_layout" android:background="@android:color/transparent" android:layout_width="fill_parent" android:layout_height="fill_parent">
        <ScrollView android:id="@id/prsent_scrollView" android:background="@android:color/transparent" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_alignParentTop="true">
            <LinearLayout android:orientation="vertical" android:paddingBottom="@dimen/game_card_margin" android:layout_width="fill_parent" android:layout_height="wrap_content">
                <RelativeLayout android:id="@id/prese_information" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/game_card_margin" android:layout_marginTop="@dimen/game_card_margin" android:layout_marginRight="@dimen/game_card_margin">
                    <RelativeLayout android:id="@id/present_details_card_title" android:background="#ffffffff" android:paddingLeft="@dimen/detail_gameinfo_padding" android:paddingTop="@dimen/game_detail_tag_title_margin_top_bottom" android:paddingRight="@dimen/detail_gameinfo_padding" android:paddingBottom="@dimen/game_detail_tag_title_margin_top_bottom" android:layout_width="wrap_content" android:layout_height="wrap_content">
                        <ImageView android:id="@id/present_game_icon" android:layout_width="@dimen/detail_gameicon_size" android:layout_height="@dimen/detail_gameicon_size" android:src="@drawable/game_list_item_default" android:layout_centerVertical="true" android:contentDescription="@string/str_icon" />
                        <TextView android:textSize="@dimen/game_name_text_size" android:textColor="#ff000000" android:ellipsize="end" android:id="@id/present_title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/game_name_margin_left" android:layout_marginBottom="@dimen/game_name_margin_bottom" android:text="礼包名称" android:singleLine="true" android:layout_toLeftOf="@id/present_get_button" android:layout_toRightOf="@id/present_game_icon" />
                        <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:id="@id/present_progress_container" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="@dimen/present_total_button_margin_left" android:layout_toLeftOf="@id/present_get_button" android:layout_toRightOf="@id/present_game_icon" android:layout_below="@id/present_title">
                            <com.youku.gamecenter.widgets.ProgressView android:id="@id/rootview" android:background="#ffd6d6d6" android:layout_width="0.0dip" android:layout_height="@dimen/progress_layout_rootview_height" android:layout_marginLeft="@dimen/game_name_margin_left" android:layout_marginRight="@dimen/game_name_margin_left" android:layout_weight="1.0">
                                <View android:id="@id/progress" android:layout_width="0.0dip" android:layout_height="@dimen/listview_gamelist_item_progress_height" />
                            </com.youku.gamecenter.widgets.ProgressView>
                            <TextView android:textSize="@dimen/present_remainder_text_size" android:textColor="#ffff6600" android:id="@id/present_remainder" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/game_present_remainder" />
                        </LinearLayout>
                        <TextView android:textSize="@dimen/list_item_des_text_size" android:textColor="#ff999999" android:ellipsize="end" android:id="@id/present_limit_time_end" android:layout_width="wrap_content" android:layout_height="@dimen/game_update_time_height" android:layout_marginLeft="@dimen/game_name_margin_left" android:text="@string/game_present_get_time_end" android:singleLine="true" android:layout_toLeftOf="@id/present_get_button" android:layout_toRightOf="@id/present_game_icon" android:layout_below="@id/present_progress_container" />
                        <Button android:textSize="@dimen/list_item_title_text_size" android:textColor="#ffffffff" android:gravity="center" android:id="@id/present_get_button" android:background="@drawable/button_blue" android:layout_width="@dimen/game_card_action_width" android:layout_height="@dimen/game_card_action_height" android:text="@string/game_present_button_get" android:layout_alignParentRight="true" android:layout_centerVertical="true" />
                    </RelativeLayout>
                    <View android:background="@color/game_card_bottom_color" android:layout_width="fill_parent" android:layout_height="@dimen/game_details_card_bttom_root_height" android:layout_below="@id/present_details_card_title" />
                </RelativeLayout>
                <RelativeLayout android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/game_card_margin" android:layout_marginTop="@dimen/game_card_margin" android:layout_marginRight="@dimen/game_card_margin">
                    <RelativeLayout android:id="@id/present_get_time_container" android:background="#ffffffff" android:paddingLeft="@dimen/detail_gameinfo_padding" android:paddingTop="@dimen/detail_gameinfo_padding" android:paddingRight="@dimen/detail_gameinfo_padding" android:paddingBottom="@dimen/detail_gameinfo_padding" android:layout_width="fill_parent" android:layout_height="wrap_content">
                        <TextView android:textSize="@dimen/game_photos_text_size" android:textColor="#ff333333" android:id="@id/present_get_time" android:layout_width="wrap_content" android:layout_height="@dimen/game_details_tv_title_height" android:text="@string/game_present_get_time_title_text" />
                        <View android:id="@id/line_time" android:background="#ffcccccc" android:layout_width="fill_parent" android:layout_height="1.0px" android:layout_below="@id/present_get_time" />
                        <TextView android:textSize="@dimen/game_category_text_size" android:textColor="#ff666666" android:id="@id/present_start_time" android:layout_width="fill_parent" android:layout_height="@dimen/game_category_height" android:layout_marginTop="@dimen/game_line_margin_top" android:text="@string/game_present_start_time" android:layout_below="@id/line_time" />
                        <TextView android:textSize="@dimen/game_category_text_size" android:textColor="#ff666666" android:id="@id/present_end_time" android:layout_width="fill_parent" android:layout_height="@dimen/game_category_height" android:layout_marginTop="@dimen/present_line_margin_top" android:text="@string/game_present_end_time" android:layout_below="@id/present_start_time" />
                    </RelativeLayout>
                    <View android:background="@color/game_card_bottom_color" android:layout_width="fill_parent" android:layout_height="@dimen/game_details_card_bttom_root_height" android:layout_below="@id/present_get_time_container" />
                </RelativeLayout>
                <RelativeLayout android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/game_card_margin" android:layout_marginTop="@dimen/game_card_margin" android:layout_marginRight="@dimen/game_card_margin">
                    <RelativeLayout android:id="@id/present_content_container" android:background="#ffffffff" android:paddingLeft="@dimen/detail_gameinfo_padding" android:paddingTop="@dimen/detail_gameinfo_padding" android:paddingRight="@dimen/detail_gameinfo_padding" android:paddingBottom="@dimen/detail_gameinfo_padding" android:layout_width="fill_parent" android:layout_height="wrap_content">
                        <TextView android:textSize="@dimen/game_photos_text_size" android:textColor="#ff333333" android:id="@id/present_content_title" android:layout_width="wrap_content" android:layout_height="@dimen/game_details_tv_title_height" android:text="@string/game_present_content_title" />
                        <View android:id="@id/line_content" android:background="#ffcccccc" android:layout_width="fill_parent" android:layout_height="1.0px" android:layout_below="@id/present_content_title" />
                        <TextView android:textSize="@dimen/game_category_text_size" android:textColor="#ff666666" android:id="@id/present_content" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/game_line_margin_top" android:text="强化绿色石＊150，装备精魄力＊300" android:layout_below="@id/line_content" android:lineSpacingMultiplier="1.5" />
                    </RelativeLayout>
                    <View android:background="@color/game_card_bottom_color" android:layout_width="fill_parent" android:layout_height="@dimen/game_details_card_bttom_root_height" android:layout_below="@id/present_content_container" />
                </RelativeLayout>
                <RelativeLayout android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/game_card_margin" android:layout_marginTop="@dimen/game_card_margin" android:layout_marginRight="@dimen/game_card_margin">
                    <RelativeLayout android:id="@id/present_limit_time_container" android:background="#ffffffff" android:paddingLeft="@dimen/detail_gameinfo_padding" android:paddingTop="@dimen/detail_gameinfo_padding" android:paddingRight="@dimen/detail_gameinfo_padding" android:paddingBottom="@dimen/detail_gameinfo_padding" android:layout_width="fill_parent" android:layout_height="wrap_content">
                        <TextView android:textSize="@dimen/game_photos_text_size" android:textColor="#ff333333" android:id="@id/present_limit_time_title" android:layout_width="wrap_content" android:layout_height="@dimen/game_details_tv_title_height" android:text="@string/game_present_limit_time_title" />
                        <View android:id="@id/line_limit_time" android:background="#ffcccccc" android:layout_width="fill_parent" android:layout_height="1.0px" android:layout_below="@id/present_limit_time_title" />
                        <TextView android:textSize="@dimen/game_category_text_size" android:textColor="#ff666666" android:id="@id/present_limit_time_content" android:layout_width="fill_parent" android:layout_height="@dimen/game_category_height" android:layout_marginTop="@dimen/game_line_margin_top" android:text="@string/game_present_limit_time" android:layout_below="@id/line_limit_time" />
                    </RelativeLayout>
                    <View android:background="@color/game_card_bottom_color" android:layout_width="fill_parent" android:layout_height="@dimen/game_details_card_bttom_root_height" android:layout_below="@id/present_limit_time_container" />
                </RelativeLayout>
                <RelativeLayout android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/game_card_margin" android:layout_marginTop="@dimen/game_card_margin" android:layout_marginRight="@dimen/game_card_margin">
                    <RelativeLayout android:id="@id/present_usage_container" android:background="#ffffffff" android:paddingLeft="@dimen/detail_gameinfo_padding" android:paddingTop="@dimen/detail_gameinfo_padding" android:paddingRight="@dimen/detail_gameinfo_padding" android:paddingBottom="@dimen/detail_gameinfo_padding" android:layout_width="fill_parent" android:layout_height="wrap_content">
                        <TextView android:textSize="@dimen/game_photos_text_size" android:textColor="#ff333333" android:id="@id/present_usage_title" android:layout_width="wrap_content" android:layout_height="@dimen/game_details_tv_title_height" android:text="@string/game_present_usage_title" />
                        <View android:id="@id/line_usage" android:background="#ffcccccc" android:layout_width="fill_parent" android:layout_height="1.0px" android:layout_below="@id/present_usage_title" />
                        <TextView android:textSize="@dimen/game_category_text_size" android:textColor="#ff666666" android:id="@id/present_usage_content" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/game_line_margin_top" android:text="玩家角色13级后点击精彩活动内礼包兑换后输入领取" android:layout_below="@id/line_usage" android:lineSpacingMultiplier="1.5" />
                    </RelativeLayout>
                    <View android:background="@color/game_card_bottom_color" android:layout_width="fill_parent" android:layout_height="@dimen/game_details_card_bttom_root_height" android:layout_below="@id/present_usage_container" />
                </RelativeLayout>
            </LinearLayout>
        </ScrollView>
    </RelativeLayout>
    <TextView android:textSize="@dimen/no_result_textsize" android:textColor="#ffb6b6b6" android:layout_gravity="center" android:id="@id/tv_no_result" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/game_no_detail_retry" android:layout_centerInParent="true" />
    <ImageView android:id="@id/iv_no_result" android:visibility="gone" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginBottom="@dimen/no_result_textsize" android:src="@drawable/pic_no_results" android:scaleType="fitCenter" android:layout_above="@id/tv_no_result" android:contentDescription="@string/str_image" />
</RelativeLayout>