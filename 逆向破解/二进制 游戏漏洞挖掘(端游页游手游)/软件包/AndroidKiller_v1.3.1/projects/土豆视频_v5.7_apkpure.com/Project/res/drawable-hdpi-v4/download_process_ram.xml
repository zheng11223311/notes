<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <gradient android:startColor="#ffd2d2d2" android:endColor="#ffd2d2d2" android:angle="270.0" android:centerY="0.75" android:centerColor="#ffd2d2d2" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <gradient android:startColor="#ffaed78b" android:endColor="#ffaed78b" android:angle="270.0" android:centerY="0.75" android:centerColor="#ffaed78b" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="#ffaed78b" android:endColor="#ffaed78b" android:angle="270.0" android:centerY="0.75" android:centerColor="#ffaed78b" />
            </shape>
        </clip>
    </item>
</layer-list>